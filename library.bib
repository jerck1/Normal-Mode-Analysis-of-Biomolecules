issn = {00219258},
journal = {J. Biol. Chem.},
number = {33},
pages = {25711--25716},
pmid = {10835424},
title = {{Molecular characterization of Vibrio parahaemolyticus vSGLT. A model for sodium-coupled sugar cotransporters}},
volume = {275},
year = {2000}
}
