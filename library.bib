isbn = {1095-9203 (Electronic)},
issn = {1095-9203},
journal = {Science},
number = {5890},
pages = {810--814},
pmid = {18599740},
title = {{The crystal structure of a sodium galactose transporter reveals mechanistic insights into Na+/sugar symport}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18599740},
volume = {321},
year = {2008}
}
@article{Amadei1993,
abstract = {Analysis of extended molecular dynamics (MD) simulations of lysozyme in vacuo and in aqueous solution reveals that it is possible to separate the configurational space into two subspaces: (1) an "essential" subspace containing only a few degrees of freedom in which anharmonic motion occurs that comprises most of the positional fluctuations; and (2) the remaining space in which the motion has a narrow Gaussian distribution and which can be considered as "physically constrained." If overall translation and rotation are eliminated, the two spaces can be constructed by a simple linear transformation in Cartesian coordinate space, which remains valid over several hundred picoseconds. The transformation follows from the covariance matrix of the positional deviations. The essential degrees of freedom seem to describe motions which are relevant for the function of the protein, while the physically constrained subspace merely describes irrelevant local fluctuations. The near-constraint behavior of the latter subspace allows the separation of equations of motion and promises the possibility of investigating independently the essential space and performing dynamic simulations only in this reduced space.},
author = {Amadei, Andrea and Linssen, Antonius B. M. and Berendsen, Herman J. C.},
doi = {10.1002/prot.340170408},
file = {:home/john/Descargas/amadei1993.pdf:pdf},
isbn = {0887-3585 (Print)$\backslash$n0887-3585 (Linking)},
issn = {0887-3585},
journal = {Proteins Struct. Funct. Genet.},
keywords = {constraint dynam-,ics,lysozyme,molecular dynamics,normal modes},
month = {dec},
number = {4},
pages = {412--425},
pmid = {8108382},
title = {{Essential dynamics of proteins}},
url = {http://doi.wiley.com/10.1002/prot.340170408},
volume = {17},
year = {1993}
}
@article{Bisha2014,
abstract = {Sodium-Galactose Transporter (SGLT) is a secondary active symporter which accumulates sugars into cells by using the electrochemical gradient of Na+ across the membrane. Previous computational studies provided insights into the release process of the two ligands (galactose and sodium ion) into the cytoplasm from the inward-facing conformation of Vibrio parahaemolyticus sodium/galactose transporter (vSGLT). Several aspects of the transport mechanism of this symporter remain to be clarified: (i) a detailed kinetic and thermodynamic characterization of the exit path of the two ligands is still lacking; (ii) contradictory conclusions have been drawn concerning the gating role of Y263; (iii) the role of Na+ in modulating the release path of galactose is not clear. In this work, we use bias-exchange metadynamics simulations to characterize the free energy profile of the galactose and Na+ release processes toward the intracellular side. Surprisingly, we find that the exit of Na+ and galactose is non-concerted as the cooperativity between the two ligands is associated to a transition that is not rate limiting. The dissociation barriers are of the order of 11-12 kcal/mol for both the ion and the substrate, in line with kinetic information concerning this type of transporters. On the basis of these results we propose a branched six-state alternating access mechanism, which may be shared also by other members of the LeuT-fold transporters.},
author = {Bisha, Ina and Rodriguez, Alex and Laio, Alessandro and Magistrato, Alessandra},
doi = {10.1371/journal.pcbi.1004017},
editor = {MacKerell, Alexander},
file = {:home/john/.local/share/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Bisha et al. - 2014 - Metadynamics Simulations Reveal a NasupsupIndependent Exiting Path of Galactose for the Inward-Facing Conformation.pdf:pdf},
issn = {15537358},
journal = {PLoS Comput. Biol.},
month = {dec},
number = {12},
pages = {e1004017},
pmid = {25522004},
title = {{Metadynamics simulations reveal a Na+ independent exiting path of galactose for the inward-facing conformation of vSGLT}},
url = {http://dx.plos.org/10.1371/journal.pcbi.1004017 http://www.ncbi.nlm.nih.gov/pubmed/25522004 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4270436},
volume = {10},
year = {2014}
}
